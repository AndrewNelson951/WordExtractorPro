{
    "manifest_version": 2,
    "name": "Word Extractor",
    "version": "1.0",
    "description": "Extracts a specified word from a web page and saves it to an Excel file.",
    "permissions": [
      "activeTab",
      "storage",
      "downloads"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ]
  }