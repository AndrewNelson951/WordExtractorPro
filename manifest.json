{
    "manifest_Version": 2,
    "name": "Word Extractor",
    "version": "1.0",
    "description": "Extracts a specified word form a web page and saves it to an Excel file.",
    "permission": [
        "activeTab",
        "storage",
        "downloads"
    ],
    "browser_action": {
        "default_popup":"popup.html"
    },
    "content_scripts":[
        {
            "matches":[
                "<all_irls>"
            ],
            "js":["content.js"]
        }
    ]
}